<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>movieController</servlet-name>
  	<display-name>movieController</display-name>
  	<description></description>
  	<servlet-class>com.stackroute.web.movieController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UDController</servlet-name>
  	<display-name>UDController</display-name>
  	<description></description>
  	<servlet-class>com.stackroute.web.UDController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>movieController</servlet-name>
  	<url-pattern>/movie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UDController</servlet-name>
  	<url-pattern>/UDController</url-pattern>
  </servlet-mapping>
</web-app>
